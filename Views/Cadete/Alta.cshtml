@{
    ViewData["Title"] = "Registrar cadete";
}

<div class="text-center">
    <h1 class="display-4">Alta de Cadete</h1>
</div>
<form method="post">
  <div class="mb-3">
    <label for="nombre" class="form-label">Nombre</label>
    <input type="text" class="form-control" id="nombre" aria-describedby="emailHelp" name="nombre">
  </div>
  <div class="mb-3">
    <label for="telefono" class="form-label">Telefono</label>
    <input type="number" class="form-control" id="telefono" name = "telefono">
  </div>
  <div class="mb-3">
    <label for="direccion" class="form-label">Direccion</label>
    <input type="text" class="form-control" id="direccion" name = "direc">
  </div>
  <input type="submit" value="Registrar">
</form>

